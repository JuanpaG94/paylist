{
  "name": "Paylist",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "build:android": "(rm -rf android/app/build) & react-native run-android",
    "build:android-nodev": "(rm -rf android/app/build) & react-native run-android --dev false",
    "build:android-release": "(rm -rf android/app/build) & react-native run-android --variant=release",
    "ios": "(rm -rf ios/build) & npx react-native run-ios --simulator='iPhone 11'",
    "ios:release": "(rm -rf ios/build) & react-native run-ios --configuration Release --simulator='iPhone 11'",
    "start": "react-native start",
    "start:reset": "react-native start --reset-cache",
    "test": "jest"
  },
  "dependencies": {
    "flow-bin": "^0.98.1",
    "react": "^16.8.3",
    "react-native": "^0.60.5",
    "react-native-color-palette": "^2.1.0",
    "react-native-firebase": "~5.5.6",
    "react-native-gesture-handler": "^1.2.1",
    "react-native-ionicons": "^4.6.3",
    "react-native-raw-bottom-sheet": "^2.0.5",
    "react-native-screens": "^1.0.0-alpha.23",
    "react-navigation": "^3.11.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/runtime": "^7.5.5",
    "babel-jest": "^24.9.0",
    "jest": "^24.9.0",
    "metro-react-native-babel-preset": "^0.54.1",
    "react-test-renderer": "16.8.3"
  },
  "jest": {
    "preset": "react-native"
  }
}
